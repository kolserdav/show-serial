{
  "name": "serial",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=production node ${npm_package_main}",
    "start:win": "set NODE_ENV=production && node index.js",
