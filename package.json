  "nodemonConfig": {
    "execMap": {
      "ejs": "javascript"
    }
  },
  "dependencies": {
    "dotenv": "^16.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.3",
    "mime-types": "^2.1.35"
  }
}
